<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.maqs.moneytracker.model.DataField" table="bnk_stmnt_data_field">
		<meta attribute="class-description">This class contains DataField details.</meta>
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>

		<property name="dataMapId" type="long" column="DATA_MAP_ID" />

		<property name="propertyName" type="string" column="PROP_NAME" />

		<property name="dataAsString" type="string" column="DATA" />

		<property name="dataType" type="string" column="DATA_TYPE" />

		<property name="userId" type="long" column="user_id" />
		<!-- <property name="createdOn" type="timestamp" column="CREATED_DATE" 
			/> <property name="lastModifiedOn" type="timestamp" column="MODIFIED_DATE" 
			/> -->
	</class>

</hibernate-mapping>