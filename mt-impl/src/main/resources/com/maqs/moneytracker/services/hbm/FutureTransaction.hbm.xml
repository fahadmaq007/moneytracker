<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	
	<class name="com.maqs.moneytracker.model.FutureTransaction" table="future_transaction">
		<meta attribute="class-description">This class contains FutureTransaction details.</meta>
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>

		<property name="transactionId" type="long" column="TRAN_ID" />
		
		<property name="periodCode" type="string" column="PERIOD" />
		
		<property name="howMany" type="int" column="HOW_MANY" />
		
		<property name="userId" type="long" column="user_id" />
		<!-- <property name="createdOn" type="timestamp"  column="CREATED_DATE" />
		
		<property name="lastModifiedOn" type="timestamp"  column="MODIFIED_DATE" /> -->
	</class>
	
</hibernate-mapping>