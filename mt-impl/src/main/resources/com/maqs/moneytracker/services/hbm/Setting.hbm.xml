<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	
	<class name="com.maqs.moneytracker.model.Setting" table="setting">
		<meta attribute="class-description">This class contains Setting details.</meta>
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>

		<property name="name" type="string" column="name" />

		<property name="description" type="string" column="description" />
		
		<property name="type" type="string" column="type" />
		
		<property name="value" type="string" column="value" />
		
		<property name="userId" type="long" column="user_id" />
		
		<!-- <property name="createdOn" type="timestamp"  column="CREATED_DATE" />
		
		<property name="lastModifiedOn" type="timestamp"  column="MODIFIED_DATE" /> -->
	</class>
	
</hibernate-mapping>